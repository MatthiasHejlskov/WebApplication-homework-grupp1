@page
@model NonTaxableDaysModel

<h1>Taxable Days</h1>

<table class="table">
    <thead>
        <tr>
            <th>Date</th>
            <th>Weekday</th>
            <th>Taxable</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model.Days)
        {
            <tr>
                <td>@item.Month</td>
                <td>@item.Day</td>
                <td>@item.TaxableDay</td>
            </tr>
        }
    </tbody>
</table>